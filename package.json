{
  "name": "managed-dataset-definitions",
  "version": "1.0.0",
  "description": "Managed Dataset Definitions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofasheville/managed-dataset-definitions.git"
  },
  "keywords": [
    "SimpliCity",
    "GraphQL"
  ],
  "author": "Eric Jackson <ejackson@ashevillenc.gov>",
  "license": "MIT",
  "homepage": "https://github.com/cityofasheville/managed-dataset-definitions",
  "dependencies": {
    "bunyan": "^1.8.5",
    "dotenv": "^2.0.0",
    "pg": "6.1.0",
    "pg-metadata": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-inline-import": "^2.0.1",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "max-len": [
        "error",
        100,
        {
          "ignoreTrailingComments": true
        }
      ]
    },
    "env": {
      "mocha": true
    }
  }
}
